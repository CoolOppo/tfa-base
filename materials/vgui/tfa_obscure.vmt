"UnlitGeneric"
{
	$basetexture "vgui/tfa_obscure_mip"
	$translucent "1"
	$ignorez "1"
	$nocull "1"
	$vertexcolor "1"
	$vertexalpha "1"
	"Proxies" 
	{ 
		"AnimatedTexture" 
		{ 
			"animatedTextureVar" "$basetexture" 
			"animatedTextureFrameNumVar" "$frame" 
			"animatedTextureFrameRate" "60" 
		}
	} 
}